{
  "name": "insightfinder",
  "version": "0.1.0",
  "description": "insightfinder web ui",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack/development.js -d --progress --colors --display-error-details",
    "devserver": "export FLASK_CONFIG=development && export WEBPACK_DEV_SERVER=1 && python manage.py runserver",
    "stage": "rm -rf ./static && webpack --config webpack/stage.js -d --progress --colors --display-error-details",
    "stageserver": "export FLASK_CONFIG=development && python manage.py runserver",
    "build": "rm -rf ./static && webpack -p  --progress --colors --config webpack/production.js",
    "start": "export FLASK_CONFIG=production && python manage.py runserver",
    "test": "export FLASK_CONFIG=testing && py.test || true",
    "fulltest": "export FLASK_CONFIG=testing && py.test --cov-report term-missing -n4 --cov backend || true"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xiaohuigu/web.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.12.0",
    "resolve-url-loader": "^1.4.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3": "^3.5.17",
    "dygraphs": "^1.1.1",
    "form-serializer": "^2.5.0",
    "jquery": "^2.2.3",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "react-timeout": "^1.0.0"
  }
}
