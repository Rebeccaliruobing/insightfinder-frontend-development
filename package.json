{
  "name": "insightfinder",
  "version": "0.1.0",
  "description": "insightfinder web ui",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server -d --progress --colors --display-error-details --devtool inline-source-map",
    "devserver": "export FLASK_CONFIG=development && export WEBPACK_DEV_SERVER=1 && python manage.py runserver",
    "devbuild": "rm -rf ./static && webpack -d --progress --colors --display-error-details --devtool inline-source-map",
    "devstart": "export FLASK_CONFIG=development && python manage.py runserver",
    "build": "rm -rf ./static && NODE_ENV=production webpack -p --progress --colors --config webpack.production.config.js",
    "start": "export FLASK_CONFIG=production && python manage.py runserver"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xiaohuigu/web.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "node-sass": "^3.4.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.4"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^15.0.0-rc.2",
    "react-dom": "^15.0.0-rc.2",
    "react-router": "^2.0.1",
    "semantic-ui": "git+ssh://git@github.com/Semantic-Org/Semantic-UI.git#next"
  }
}
